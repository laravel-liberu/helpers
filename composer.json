{
    "name": "laravel-liberu/helpers",
    "description": "Helper classes dependency for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "helpers"
    ],
    "homepage": "https://github.com/laravel-liberu/helpers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Helpers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "EnsoException": "LaravelEnso\\Helpers\\Exceptions\\EnsoException"
            }
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
